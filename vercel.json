{
    "cleanUrls": true,
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "x-robots-tag",
            "value": "index, follow"
          }
        ]
      },
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, must-revalidate"
          }
        ]
      }
    ]
  }
  